 "compilerOptions": {
    "baseUrl": ".", 
    "paths": "@/*": ["./src/*"]
    "outDir": "./dist",
    "rootDir": "./src",
    "tsBuildInfoFile": "./node_modules/.cache/tsconfig.app.tsbuildinfo",
            "target": "ES2020",
            "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.jsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.lib.json" },
    { "path": "./tsconfig.schematics.json" },
    { "path": "./tsconfig.spec.json" },
    { "path": "./tsconfig.json" },
    { "path": "./tsconfig.types.json" },
    { "path": "./tsconfig.worker.json" },
    { "path": "./tsconfig.worker.spec.json" },
    { "path": "./tsconfig.worker.types.json" },
    { "path": "./tsconfig.worker.spec.types.json" },
    { "path": "./tsconfig.worker.lib.json" },
    { "path": "./tsconfig.worker.lib.types.json" },
    { "path": "./tsconfig.worker.lib.spec.json" },
    { "path": "./tsconfig.worker.lib.spec.types.json" },
    { "path": "./tsconfig.worker.schematics.json" },
    { "path": "./tsconfig.worker.schematics.types.json" },
    { "path": "./tsconfig.worker.schematics.spec.json" },
    { "path": "./tsconfig.worker.schematics.spec.types.json" },
    { "path": "./tsconfig.worker.spec.json" },
    { "path": "./tsconfig.worker.spec.types.json" },
    { "path": "./tsconfig.worker.spec.schematics.json" },
    { "path": "./tsconfig.worker.spec.schematics.types.json" },
    { "path": "./tsconfig.worker.spec.lib.json" },
    { "path": "./tsconfig.worker.spec.lib.types.json" },
    { "path": "./tsconfig.worker.spec.lib.schematics.json" },
    { "path": "./tsconfig.worker.spec.lib.schematics.types.json" },
    { "path": "./tsconfig.worker.spec.lib.worker.json" },
    { "path": "./tsconfig.worker.spec.lib.worker.types.json" },
    { "path": "./tsconfig.worker.spec.lib.worker.schematics.json" },
    { "path": "./tsconfig.worker.spec.lib.worker.schematics.types.json" },
    { "path": "./tsconfig.worker.spec.lib.worker.spec.json" },
    { "path": "./tsconfig.worker.spec.lib.worker.spec.types.json" },
    { "path": "./tsconfig.worker.spec.lib.worker.schematics.lib.json" }